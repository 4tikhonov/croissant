{
  "@context": {
    "@vocab": "https://schema.org/",
    "applyTransform": "ml:applyTransform",
    "csvColumn": "ml:csvColumn",
    "data": {
      "@id": "ml:data",
      "@nest": "source"
    },
    "dataExtraction": "ml:dataExtraction",
    "dataType": {
      "@id": "ml:dataType",
      "@type": "@vocab"
    },
    "field": "ml:field",
    "fileProperty": "ml:fileProperty",
    "format": "ml:format",
    "includes": "ml:includes",
    "inlineData": {
      "@id": "ml:data",
      "@type": "@json"
    },
    "jsonPath": "ml:jsonPath",
    "ml": "http://mlcommons.org/schema/",
    "path": "ml:path",
    "recordSet": "ml:recordSet",
    "references": "ml:references",
    "regex": "ml:regex",
    "replace": "ml:replace",
    "sc": "https://schema.org/",
    "separator": "ml:separator",
    "source": "ml:source",
    "subField": "ml:subField",
    "wd": "https://www.wikidata.org/wiki/"
  },
  "@type": "sc:Dataset",
  "name": "Mini-PASS",
  "description": "Smaller downloadable version of PASS to be used in unit tests.",
  "@language": "en",
  "citation": "None",
  "license": "None",
  "url": "None",
  "distribution": [
    {
      "@type": "sc:FileObject",
      "name": "metadata",
      "contentUrl": "data/pass_metadata.csv",
      "encodingFormat": "text/csv",
      "sha256": "foo"
    },
    {
      "@type": "sc:FileObject",
      "name": "pass0",
      "contentUrl": "data/PASS.0.tar",
      "encodingFormat": "application/x-tar",
      "sha256": "bar"
    },
    {
      "@type": "sc:FileObject",
      "name": "pass1",
      "contentUrl": "data/PASS.1.tar",
      "encodingFormat": "application/x-tar",
      "sha256": "bar"
    },
    {
      "@type": "sc:FileSet",
      "name": "image-files",
      "containedIn": [
        "#{pass0}",
        "#{pass1}"
      ],
      "encodingFormat": "image/jpeg",
      "includes": "*.jpg"
    }
  ],
  "recordSet": [
    {
      "@type": "ml:RecordSet",
      "name": "images",
      "key": "hash",
      "field": [
        {
          "@type": "ml:Field",
          "name": "hash",
          "description": "The hash of the image, as computed from YFCC-100M.",
          "dataType": "sc:Text",
          "references": {
            "dataExtraction": {
              "csvColumn": "hash"
            },
            "distribution": "metadata"
          },
          "source": {
            "applyTransform": {
              "regex": "([^\\/]*)\\.jpg"
            },
            "dataExtraction": {
              "fileProperty": "filename"
            },
            "distribution": "image-files"
          }
        },
        {
          "@type": "ml:Field",
          "name": "image_content",
          "description": "The content of the image.",
          "dataType": "sc:ImageObject",
          "source": {
            "dataExtraction": {
              "fileProperty": "content"
            },
            "distribution": "image-files"
          }
        },
        {
          "@type": "ml:Field",
          "name": "creator_uname",
          "description": "Unique name of photo creator.",
          "dataType": "sc:Text",
          "source": {
            "dataExtraction": {
              "csvColumn": "unickname"
            },
            "distribution": "metadata"
          }
        },
        {
          "@type": "ml:Field",
          "name": "gps_coordinates",
          "description": "GPS coordinates where the image was taken.",
          "dataType": "sc:GeoCoordinates",
          "subField": [
            {
              "@type": "ml:Field",
              "name": "latitude",
              "dataType": "sc:Float",
              "source": {
                "dataExtraction": {
                  "csvColumn": "latitude"
                },
                "distribution": "metadata"
              }
            },
            {
              "@type": "ml:Field",
              "name": "longitude",
              "dataType": "sc:Float",
              "source": {
                "dataExtraction": {
                  "csvColumn": "longitude"
                },
                "distribution": "metadata"
              }
            }
          ]
        },
        {
          "@type": "ml:Field",
          "name": "date_taken",
          "description": "The date the photo was taken.",
          "dataType": "sc:Date",
          "source": {
            "applyTransform": {
              "format": "%Y-%m-%d %H:%M:%S.%f"
            },
            "dataExtraction": {
              "csvColumn": "datetaken"
            },
            "distribution": "metadata"
          }
        }
      ]
    }
  ]
}
