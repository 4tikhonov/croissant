{
  "@context": {
    "@vocab": "https://schema.org/",
    "sc": "https://schema.org/",
    "ml": "http://mlcommons.org/schema/",
    "includes": "ml:includes",
    "recordSet": "ml:RecordSet",
    "field": "ml:Field",
    "dataType": "ml:dataType",
    "source": "ml:source",
    "references": "ml:references"
  },
  "@type": "sc:Dataset",
  "name": "Enum example",
  "description": "This is a fairly minimal example, showing a way to describe enumerations.",
  "url": "https://example.com/datasets/enum/about",
  "distribution": [
    {
      "@type": "sc:FileObject",
      "name": "enum.csv",
      "contentUrl": "https://example.com/datasets/recipes/enum.csv",
      "sha256": "52121a5df209dc2d63069710923e25f769d380a1388d00798e84d432c54fd934",
      "encodingFormat": "text/csv"
    }
  ],
  "recordSet": [
    {
      "@type": "ml:RecordSet",
      "name": "direction_enum",
      "description": "Maps compass direction keys (0, 1, 2, 3) to labeled values.",
      "key": "#{key}",
      "field": [
          {
            "name": "key",
            "description": "0, 1, 2 or 3",
            "@type": "ml:Field",
            "dataType": "sc:Integer"
          },
          {
            "name": "label",
            "description": "North, East, South or West",
            "@type": "ml:Field",
            "dataType": ["sc:Text", "sc:name"]
          }
      ],
      "itemListElement": [
        {
          "@type": "ListItem",
          "item": [
            {
              "@type": "PropertyValue",
              "name": "key",
              "value": 0
            },
            {
              "@type": "PropertyValue",
              "name": "label",
              "value": "North"
            }
          ]
        },{
          "@type": "ListItem",
          "item": [
            {
              "@type": "PropertyValue",
              "name": "key",
              "value": 1
            },
            {
              "@type": "PropertyValue",
              "name": "label",
              "value": "East"
            }
          ]
        },{
          "@type": "ListItem",
          "item": [
            {
              "@type": "PropertyValue",
              "name": "key",
              "value": 2
            },
            {
              "@type": "PropertyValue",
              "name": "label",
              "value": "South"
            }
          ]
        },{
          "@type": "ListItem",
          "item": [
            {
              "@type": "PropertyValue",
              "name": "key",
              "value": 3
            },
            {
              "@type": "PropertyValue",
              "name": "label",
              "value": "West"
            }
          ]
        }
      ]
    },
    {
      "@type": "ml:RecordSet",
      "name": "examples",
      "source": "#{enum.csv}",
      "description": "Records extracted from the examples table, with their schema.",
      "field": [
        {
          "name": "name",
          "@type": "ml:Field",
          "dataType": "sc:Text",
          "description": "The first column contains the name of the object.",
          "references": "#{enum.csv/name}"
        },
        {
          "name": "direction",
          "@type": "ml:Field",
          "dataType": ["sc:Text", "ml:Enum"],
          "description": "Direction of the object (0: North, 1: East, 2: South, 3: West)",
          "source": "#{enum.csv/direction}",
          "references": "#{direction_enum/key}"
        }
      ]
    }
  ]
}